{
  "name": "probet-pro-server",
  "version": "1.0.0",
  "description": "Professional sports betting calculator with advanced mathematical models and secure API management",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "build": "npm run lint && npm test",
    "docker:build": "docker build -t probet-pro .",
    "docker:run": "docker run -p 3000:3000 probet-pro"
  },
  "keywords": [
    "sports-betting",
    "mathematical-models",
    "kelly-criterion",
    "risk-management",
    "api",
    "nodejs"
  ],
  "author": "ProBet Pro Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^1.6.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mathjs": "^11.11.0",
    "ml-matrix": "^6.10.4",
    "node-cron": "^3.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.8",
    "sequelize": "^6.32.1",
    "simple-statistics": "^7.8.3",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/probet-pro/server.git"
  },
  "bugs": {
    "url": "https://github.com/probet-pro/server/issues"
  },
  "homepage": "https://github.com/probet-pro/server#readme"
}
